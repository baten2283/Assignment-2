def max_min_finder(numbers):
    max_num = numbers[0]
    min_num = numbers[0]

    for num in numbers:
        if num > max_num:
            max_num = num
        elif num < min_num:
            min_num = num
    
    return max_num, min_num

number_list = [2,3,4,64,7,9,87,98,12,34,51]
max_number, min_number = max_min_finder(number_list)
print("Maximum number is", max_number)
print("Minimum number is", min_number)
