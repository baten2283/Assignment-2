def swap_elements(my_list):
    new_list = []

    for tup in my_list:
        if len(tup) < 2:
            new_list.append(tup)
        else:
            new_tuple = (tup[-1],) + (tup[1:-1]) + (tup[0],)
            new_list.append(new_tuple)
    return new_list

list_num = [(1,3,5,2), (6,4,8,2,9),(2,5)]
result = swap_elements(list_num)
print(result)
